{
  "name": "microlib-utils",
  "version": "1.0.0",
  "description": "Utility libary for other MicroLib libraries.",
  "main": "src/utils.microlib.js",
  "keywords": [
    "utils",
    "micro",
    "lightweight"
  ],
  "scripts": {
