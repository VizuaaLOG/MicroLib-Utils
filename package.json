  },
  "author": "Thomas Erbe <vizuaalog@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "jshint": "^2.9.2",
    "rollup": "^0.26.3",
    "rollup-plugin-buble": "^0.10.0",
    "uglify-js": "^2.6.2"
  }
}
